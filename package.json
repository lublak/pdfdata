  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "@types/opentype.js": "^1.3.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "canvas": "^2.11.2",
    "eslint": "^8.57.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.2",
    "jest": "^29.7.0",
    "pureimage": "^0.4.13",
    "tesseract.js": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
