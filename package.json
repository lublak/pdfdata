{
  "name": "pdfdataextract",
  "version": "3.2.0",
  "description": "Extract data from a pdf with pure javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" --cache",
    "lint:fix": "eslint \"src/**/*.ts\" --cache --fix",
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "build:doc": "typedoc src/index.ts"
  },
  "keywords": [
    "pdf",
    "pdf.js",
    "pdfjs",
    "parse",
    "parser",
    "json",
    "text",
    "data",
    "crawler",
    "extract",
    "extractor",
    "ocr",
    "converter",
    "image"
  ],
  "dependencies": {
    "pdfjs-dist": "^2.14.305"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/opentype.js": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "canvas": "^2.9.3",
    "eslint": "^8.20.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "jest": "^28.1.3",
    "pureimage": "^0.3.14",
    "tesseract.js": "^2.1.5",
    "ts-jest": "^28.0.7",
    "typedoc": "^0.23.8",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "^4.7.4"
  },
  "homepage": "https://github.com/lublak/pdfdataextract#readme",
  "bugs": {
    "url": "https://github.com/lublak/pdfdataextract/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lublak/pdfdataextract.git"
  },
  "author": "lublak",
  "license": "MIT"
}
