{
  "name": "pdfdataextract",
  "version": "4.0.0",
  "description": "Extract data from a pdf with pure javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" --cache",
