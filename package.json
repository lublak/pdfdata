{
  "name": "pdfdataextract",
  "version": "3.2.0",
  "description": "Extract data from a pdf with pure javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" --cache",
    "lint:fix": "eslint \"src/**/*.ts\" --cache --fix",
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "build:doc": "typedoc src/index.ts"
  },
  "keywords": [
    "pdf",
    "pdf.js",
    "pdfjs",
    "parse",
    "parser",
    "json",
    "text",
    "data",
    "crawler",
    "extract",
    "extractor"
  ],
  "dependencies": {
    "pdfjs-dist": "^2.12.313"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^14.18.2",
    "@types/opentype.js": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "canvas": "^2.8.0",
    "eslint": "^7.32.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-jsdoc": "^33.3.0",
    "jest": "^27.4.7",
    "pureimage": "^0.3.8",
    "tesseract.js": "^2.1.5",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.10",
    "typedoc-github-wiki-theme": "^0.6.0",
    "typedoc-plugin-markdown": "^3.11.8",
    "typescript": "^4.5.4"
  },
  "homepage": "https://github.com/lublak/pdfdataextract#readme",
  "bugs": {
    "url": "https://github.com/lublak/pdfdataextract/issues"
  },
  "repository": {
    "type": "git",
